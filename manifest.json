{
  "manifest_version": 3,
  "name": "Quran Tab",
  "version": "1.2.2",
  "description": "Beautiful Quran verses in every new tab",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}